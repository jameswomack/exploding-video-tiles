{
  "env": {
    "es6": true,
    "worker": true,
    "node": true,
    "browser": true,
    "builtin": true
  },
  "ecmaFeatures": {
    "objectLiteralDuplicateProperties": true,
    "destructuring": true,
    "defaultParams": true,
    "classes": true,
    "arrowFunctions": true,
    "modules": true
  },
  "globals": {
    "d3": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-style": [
      2,
      "last"
    ],
    "curly": 0,
    "dot-notation": [
      2,
      {
        "allowKeywords": true
      }
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "guard-for-in": 2,
    "key-spacing": 0,
    "new-cap": 0,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 0,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-expressions": 0,
    "no-unused-vars": 2,
    "no-underscore-dangle": 0,
    "no-with": 2,
    "no-multiple-empty-lines": [
      2, {"max": 3, "maxEOF": 1}
    ],
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "no-var": 0,
    "quotes": 0,
    "semi": 0,
    "semi-spacing": 0,
    "strict": 0,
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ]
  }
}
